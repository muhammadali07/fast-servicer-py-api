# container
COMPOSE_PROJECT_NAME=assessment-unife
PROJECT_NETWORK="assessment-net" # need create new network in docker for first time
PROJECT_NAME=assessment-unife
PROJECT_NAME_API=OPEN_API_ASSESSMENT
CONTAINER_ID_NAME="assessment" 

# Postgres API DB
POSTGRES_DB_SERVER="db" # same with keycloack-db
POSTGRES_DB_PORT=5432
POSTGRES_DB_EXPOSE_PORT=5439
POSTGRES_DB_API=assessment_dev
POSTGRES_DB_API_USER=assessment_user
POSTGRES_DB_API_PASSWORD=assessment_pass

# api
API_HTTP_PORT=80
API_HTTP_EXPOSE_PORT=8888
PROXY_ROOT_PATH=/unife
API_VERSION=/api/v1

#service api
# URL_API=http://localhost:8888/api/v1/acc